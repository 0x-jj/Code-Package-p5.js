{
  "name": "Code-Package-p5-js",
  "version": "0.0.0",
  "description": "Code package of the book Generative Design for p5.js",
  "scripts": {
    "start": "npm run watch",
    "watch": "budo --dir . --open --live --watch-glob '**/*.{html,css,js}'",
    "setup": "cd libraries && node setup.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/generative-design/Code-Package-p5.js.git"
  },
  "keywords": [
    "processing",
    "p5",
    "generative",
    "computational",
    "design"
  ],
  "author": "Benedikt Gro√ü <bg@benedikt-gross.de>",
  "bugs": {
    "url": "https://github.com/generative-design/Code-Package-p5.js/issues"
  },
  "homepage": "http://www.generative-gestaltung.de",
  "devDependencies": {
    "budo": "^9.2.2",
    "fs-sync": "^1.0.4",
    "g.js": "^0.1.2",
    "generative-design-library": "git@github.com:generative-design/generative-design-library.js.git#18e32cc0abd487845e46a335560e0061bb2970e7",
    "glob": "^7.1.1",
    "opentype.js": "^0.6.8",
    "p5": "^0.5.7",
    "gif.js": "^0.2.0",
    "file-saver": "^1.3.3"
  },
  "install": {
    "sources": {
      "p5": [
        "node_modules/p5/lib/p5.js",
        "node_modules/p5/lib/addons/p5.dom.js"
      ],
      "generative-design-library": [
        "node_modules/generative-design-library.js/dist/generative-design-library.js"
      ],
      "g.js": [
        "node_modules/g.js/dist/g.js"
      ],
      "opentype.js": [
        "node_modules/opentype.js/dist/opentype.js"
      ],
      "gif.js": [
        "node_modules/gif.js/dist/gif.js",
        "node_modules/gif.js/dist/gif.worker.js"
      ],
      "filesaver": [
        "node_modules/file-saver/FileSaver.js"
      ]
    }
  }
}
